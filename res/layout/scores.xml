                android:textSize="25sp"
                android:paddingRight="20dp"
                android:text="" />

            <TextView
                android:id="@+id/score_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="15dp"
                android:textSize="25sp"
                android:text="" />

